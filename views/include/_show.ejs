<% for(var i=0; i<users.length; i++){ %> 
<tr>
    <td><%= i + 1 %> </td>
    <td><%= users[i].first_name %> </td>
    <td><%= users[i].last_name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].address1 %></td>
    <td><%= users[i].address2 %></td>
    <td><%= users[i].city %></td>
    <td><%= users[i].postal_code %></td>
    <td><%= users[i].country %></td>
    <td><%= users[i].birthday %></td>
    <td><%= users[i].phone_number %></td>
    <td><%= users[i].user_notes %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
       <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
       </a>
        
    </td>
</tr>
<% } %> 